<Page x:Class="pr28.Pages.Clubs.Club"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:pr28.Pages.Clubs"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Club" Background="#201d1d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Border BorderBrush="#00f52a" BorderThickness="0,0,0,2">
            <TextBlock Text="Компьютерные клубы города Пермь" Style="{StaticResource mainTbStyle}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Border>
        <ScrollViewer Grid.Row="1" Background="#312f2f">
            <StackPanel x:Name="parent">
            </StackPanel>
        </ScrollViewer>
    </Grid>
</Page>
